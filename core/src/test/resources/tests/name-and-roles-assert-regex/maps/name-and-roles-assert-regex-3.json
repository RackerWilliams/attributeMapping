 {
  "rules": [
     {
      "remote": [
         {
          "name":"UserName"
         },
         {
          "name":"Email"
         },
         {
          "name":"ExpireAfter"
         },
         {
          "path":"\/\/saml2:Assertion\/saml2:Subject\/saml2:NameID",
          "regex":true,
          "notAnyOf": [".*downer$"]
         },
         {
          "path":"\/\/saml2:Assertion\/saml2:Subject\/saml2:NameID",
          "regex":true,
          "anyOneOf":".*upper$"
         }
       ],
      "local": {
        "user": {
          "domain":"78334",
          "name":"{0}",
          "email":"{1}",
          "expire":"{2}",
          "roles": [
            "nova:admin",
            "lbaas:admin"
           ]
         }
       }
     }
   ]
 }
